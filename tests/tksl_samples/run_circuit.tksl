
use pytket::{compile_circuits, substitute_symbols, execute, load_qasm};

// not needed anymore
Graph zip(l1: Vector<Str>, l2: Vector<Int>, N: Int) -> (zipped:Vector<Pair<Str,Int>>) {
    looped <- while (outp: [], first: l1, second: l2, counter: N) {
        output (pred: python_nodes::less_than(0, counter));
    } do {
        pop1 <- pop(first);
        pop2 <- pop(second);
        pushed <- push(outp, make_pair(pop1.item, pop2.item));
        output (outp: pushed.vec,
                first: pop1.vec,
                second: pop2.vec,
                counter: python_nodes::add(counter, -1));
    };

    output (zipped: looped.outp);

}

type SampledDist = Pair<Map<Str, Float>, Int>;

Graph main() -> (res: SampledDist) {
    const qasm_str = read_file!("tests/tksl_samples/simple_qasm.qasm");
    subs <- substitute_symbols(load_qasm(qasm_str), ["a_1"], [0.1]);
    comped <- compile_circuits(push([], subs), "RebaseTket");
    output (res: execute(comped, [10], "AerBackend"));
}
